dictionary KeyAgent {
    User user;
    boolean verified;
    sequence<SignerOffering> signer_offerings;
    boolean is_contact;
};

dictionary SignerOffering {
    Temperature temperature;
    u16 response_time;
    DeviceType device_type;
    Price? cost_per_signature;
    u64? yearly_cost_basis_points;
    Price? yearly_cost;
    Network network;
};

interface Price {
    [Throws=FFIError]
    constructor(u64 amount, string currency);
    u64 amount();
    string currency();
};

enum Temperature {
    "Warm",
    "Cold",
    "AirGapped",
    "Unknown",
};

enum DeviceType {
    "Coldcard",
    "BitBox02",
    "Ledger",
    "Mobile",
    "Desktop",
    "CloudBased",
    "Undisclosed",
    "Unknown",
};